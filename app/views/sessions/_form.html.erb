<%= form_with url: sessions_path, local: true, data: { turbo: false } do |form| %>
  <%= flash_messages %>

  <p>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </p>

  <p>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </p>

  <p>
    <%= form.submit 'Enter' %>
  </p>
<% end %>
